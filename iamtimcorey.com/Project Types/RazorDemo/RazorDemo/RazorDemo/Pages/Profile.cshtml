@page
@model RazorDemo.Pages.ProfileModel
@{
}

<h1>Profile</h1>
<form method="post">
    <div class="form-group mt-3">
        <label asp-for="Id" class="form-label"></label>
        <input asp-for="Id" placeholder="Id number" class="form-control"/>
    </div>
    <div class="form-group mt-3">
        <label asp-for="Name" class="form-label"></label>
        <input asp-for="Name" placeholder="Name" class="form-control"/>
    </div>
    <div class="form-group mt-3">
        <label asp-for="Email" class="form-label"></label>
        <input asp-for="Email" placeholder="Email" class="form-control"/>
    </div>
    <div class="mt-3 text-end">
        <button type="submit" class="btn btn-outline-primary">Send Form</button>
    </div>
</form>
